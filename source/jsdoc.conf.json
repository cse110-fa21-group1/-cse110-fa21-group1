{
    "source": {
        "include": ["source"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": ["(^|\\/|\\\\)_", "jsdocs", "node_modules"]
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "destination": "source/jsdocs",
        "recurse": true
    }
}
